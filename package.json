{
  "homepage": "https://vsdev97.github.io/web-app",
  "name": "web-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
